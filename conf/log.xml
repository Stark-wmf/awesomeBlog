<seelog minlevel="trace">
    <outputs formatid="common">
        <filter levels="info,debug">
            <rollingfile type="size" filename="logs/access.log" maxsize="102400" maxrolls="5"/>
        </filter>
        <filter levels="error,critical">
            <rollingfile type="size" filename="logs/error.log" maxsize="102400" maxrolls="5"/>
        </filter>
    </outputs>
    <formats>
        <format id="common" format="%Date %Time [%LEV] [%File:%Line] [%Func] %Msg%n" />
    </formats>
</seelog>